             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:CoinBag.Views"
             Title="{Binding Title}"
             x:Class="CoinBag.Views.BackupWalletView">
    <views:BackupWalletViewPage.Content>
        <StackLayout>
            <Button Text="Backup" Command="{Binding BackupCommand}" />
        </StackLayout>
    </views:BackupWalletViewPage.Content>
</views:BackupWalletViewPage>